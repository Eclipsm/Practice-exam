int main(){
    for(int i = 0; i < 3; i++){
        for(int j = 0; j < 1; j++){
            printf("   *");
        }
        printf("\n");
    }
    for(int i = 0; i< 1 ; i++){
        for(int j = 0; j< 7; j++){
            printf("*");
        }
    }
    printf("\n");
     for(int i = 0; i < 3; i++){
        for(int j = 0; j < 1; j++){
            printf("   *");
        }
        printf("\n");
    }
}

