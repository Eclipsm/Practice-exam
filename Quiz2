int main(){
    for(int i = 0; i < 6; i++){
        for(int j = 0; j < 6+i; j++){
            printf("  ");
        }
        for(int k = 0; k<2*(6-i)-1 ; k++){
            printf("* ");
        }
        printf("\n");
    }
}

              * * * * * * * * * * * 
                * * * * * * * * * 
                  * * * * * * * 
                    * * * * * 
                      * * * 
                        * 

int main(){
    for(int i = 0; i < 6; i++){
        for(int j = 0; j < 6-i; j++){
            printf("  ");
        }
        for(int k = 0; k<2*i+1; k++){
            printf("* ");
        }
        printf("\n");
    }
    for(int i = 1; i < 6; i++){
        for(int j = 0; j<i+1;j++){
            printf("  ");
        }
        for(int k = 0; k<2*(6-i)-1; k++){
            printf("* ");
        }
        printf("\n");
    }
}
